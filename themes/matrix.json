          "string.regex": {
            "color": "#82d967"
          },
          "string.special": {
            "color": "#ffd700"
          },
          "string.special.symbol": {
            "color": "#ffd700"
          },
          "tag": {
            "color": "#82d967"
          },
          "text.literal": {
            "color": "#ffd700"
          },
          "title": {
            "color": "#82d967",
            "font_weight": 600
          },
          "type": {
            "color": "#82d967"
          },
          "property": {
            "color": "#88FF88"
          },
          "variable.special": {
            "color": "#FF0000"
          }
        }
