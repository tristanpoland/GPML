          "comment": {
            "color": "#007700",
            "font_style": "italic"
          },
          "comment.doc": {
            "color": "#007700",
            "font_style": "italic"
          },
          "constant": {
            "color": "#ffd700"
          },
          "constructor": {
            "color": "#82d967"
          },
          "embedded": {
